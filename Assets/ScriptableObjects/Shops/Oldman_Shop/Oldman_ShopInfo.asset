  ShopName: "\u0421\u0442\u0430\u0440\u0438\u043A"
  BackgroundImage: {fileID: 21300000, guid: 1f39e069310137b42802a028ce6d92e3, type: 3}
  Shopkeeper:
    TalkSpeeches:
    - Lines:
      - "\u0417\u0430\u0433\u043B\u044F\u043D\u0443\u043B \u0432\u0441\u0435 \u0436\u0435?
        \u0420\u0430\u0434 \u0442\u0435\u0431\u044F \u0432\u0438\u0434\u0435\u0442\u044C."
      - "\u0422\u043E\u0432\u0430\u0440\u044B \u0443 \u043C\u0435\u043D\u044F \u0440\u0430\u0437\u043D\u044B\u0435:
        \u043E\u0442 \u0437\u0435\u043B\u0438\u0439 \u0434\u043E \u044D\u043A\u0438\u043F\u0438\u0440\u043E\u0432\u043A\u0438."
      - "\u0422\u0430\u043A\u0436\u0435 \u043F\u0440\u043E\u0434\u0430\u044E \u0438\u043D\u0441\u0442\u0440\u0443\u043C\u0435\u043D\u0442\u044B
        - \u043D\u0438\u043A\u043E\u0433\u0434\u0430 \u043D\u0435 \u0437\u043D\u0430\u0435\u0448\u044C,
        \u0447\u0442\u043E \u0438\u0437 \u043D\u0438\u0445 \u043C\u043E\u0436\u0435\u0442
        \u043F\u0440\u0438\u0433\u043E\u0434\u0438\u0442\u044C\u0441\u044F."
      - "\u041E\u0441\u043C\u043E\u0442\u0440\u0438 \u0442\u043E\u0432\u0430\u0440,
        \u0432\u0434\u0440\u0443\u0433 \u0447\u0442\u043E \u043F\u0440\u0438\u0433\u043B\u044F\u043D\u0435\u0442\u0441\u044F."
      Repeatable: 0
    - Lines:
      - "\u041D\u0430 \u044D\u0442\u043E\u043C \u044D\u0442\u0430\u0436\u0435 \u0442\u0435\u0431\u044F
        \u043C\u043E\u0433\u0443\u0442 \u043F\u043E\u0434\u0441\u0442\u0435\u0440\u0435\u0433\u0430\u0442\u044C
        \u0440\u0430\u0437\u043D\u044B\u0435 \u043E\u043F\u0430\u0441\u043D\u043E\u0441\u0442\u0438."
      - "\u0422\u044B \u0442\u043E\u0447\u043D\u043E \u0432\u0441\u0442\u0440\u0435\u0442\u0438\u0448\u044C
        \u0441\u0442\u0440\u0430\u0436\u043D\u0438\u043A\u043E\u0432, \u043B\u043E\u0432\u0443\u0448\u043A\u0438
        \u0438 \u043C\u0430\u043B\u043E \u043B\u0438 \u0447\u0442\u043E \u0435\u0449\u0435."
      - "\u0411\u0443\u0434\u044C \u0433\u043E\u0442\u043E\u0432 \u043A\u043E \u0432\u0441\u0435\u043C\u0443."
      Repeatable: 1
    - Lines:
      - "\u0422\u0449\u0430\u0442\u0435\u043B\u044C\u043D\u043E \u043E\u0441\u043C\u0430\u0442\u0440\u0438\u0432\u0430\u0439
        \u043E\u043A\u0440\u0435\u0441\u0442\u043D\u043E\u0441\u0442\u0438."
      - "\u0422\u0430\u043A\u0438\u043C \u043E\u0431\u0440\u0430\u0437\u043E\u043C
        \u043D\u0435 \u0443\u043F\u0443\u0441\u0442\u0438\u0448\u044C \u043D\u0438\u0447\u0435\u0433\u043E
        \u043F\u043E\u0442\u0435\u043D\u0446\u0438\u0430\u043B\u044C\u043D\u043E
        \u043F\u043E\u043B\u0435\u0437\u043D\u043E\u0433\u043E."
      - "\u0421\u043E\u0432\u0435\u0442\u0443\u044E \u0442\u0430\u043A\u0436\u0435
        \u0432\u0441\u0435\u0433\u0434\u0430 \u043F\u0440\u0438 \u0441\u0435\u0431\u0435
        \u0434\u0435\u0440\u0436\u0430\u0442\u044C \u043F\u0430\u0440\u043E\u0447\u043A\u0443
        \u0438\u043D\u0441\u0442\u0440\u0443\u043C\u0435\u043D\u0442\u043E\u0432."
      - "\u041E\u043D\u0438 \u0442\u043E\u0447\u043D\u043E \u0433\u0434\u0435-\u043D\u0438\u0431\u0443\u0434\u044C
        \u0434\u0430 \u043F\u0440\u0438\u0433\u043E\u0434\u044F\u0442\u0441\u044F."
      Repeatable: 1
    WelcomeLines:
    - "\u0414\u043E\u0431\u0440\u043E \u043F\u043E\u0436\u0430\u043B\u043E\u0432\u0430\u0442\u044C!"
    - "\u041F\u0440\u0438\u0432\u0435\u0442\u0441\u0442\u0432\u0443\u044E."
    - "\u0412\u043E\u0442 \u0438 \u0442\u044B."
    FarewellLines:
    - "\u0411\u0435\u0440\u0435\u0433\u0438 \u0441\u0435\u0431\u044F."
    - "\u041F\u0440\u0438\u0445\u043E\u0434\u0438 \u0435\u0449\u0435."
    - "\u0412\u043E\u0437\u0432\u0440\u0430\u0449\u0430\u0439\u0441\u044F."
    PurchaseLines:
    - "\u0411\u043B\u0430\u0433\u043E\u0434\u0430\u0440\u044E."
    - "\u041D\u0430\u0434\u0435\u044E\u0441\u044C, \u0442\u0435\u0431\u0435 \u044D\u0442\u043E
      \u043F\u0440\u0438\u0433\u043E\u0434\u0438\u0442\u0441\u044F."
    - "\u0411\u0435\u0440\u0438, \u043D\u0435 \u0441\u0442\u0435\u0441\u043D\u044F\u0439\u0441\u044F."
    SellLines:
    - "\u041F\u0440\u0438\u0433\u043E\u0434\u0438\u0442\u0441\u044F."
    - "\u0411\u0435\u0440\u0443."
    - "\u0412\u043E\u0437\u044C\u043C\u0443 \u043D\u0430 \u043F\u0435\u0440\u0435\u043F\u0440\u043E\u0434\u0430\u0436\u0443."
    SellFailureLines:
    - "\u0422\u0430\u043A\u043E\u0435 \u044F \u043D\u0435 \u043A\u0443\u043F\u043B\u044E."
    - "\u042F \u043D\u0435 \u0431\u0443\u0434\u0443 \u044D\u0442\u043E \u043F\u043E\u043A\u0443\u043F\u0430\u0442\u044C."
    - "\u041C\u043D\u0435 \u044D\u0442\u043E \u043D\u0435 \u043D\u0443\u0436\u043D\u043E."
    NotEnoughMoneyLines:
    - "\u0422\u0435\u0431\u0435 \u043D\u0435 \u0445\u0432\u0430\u0442\u0430\u0435\u0442
      \u0434\u0435\u043D\u0435\u0433."
    - "\u041A\u0443\u043F\u0438 \u0447\u0442\u043E-\u043D\u0438\u0431\u0443\u0434\u044C
      \u0434\u0440\u0443\u0433\u043E\u0435."
    - "\u0412\u043E\u0437\u0432\u0440\u0430\u0449\u0430\u0439\u0441\u044F, \u043A\u043E\u0433\u0434\u0430
      \u0431\u0443\u0434\u0435\u0442 \u0434\u043E\u0441\u0442\u0430\u0442\u043E\u0447\u043D\u043E
      \u0434\u0435\u043D\u0435\u0433."
    NoPlaceLines:
    - "\u0423 \u0442\u0435\u0431\u044F \u043D\u0435\u0442 \u043C\u0435\u0441\u0442\u0430
      \u0434\u043B\u044F \u0442\u043E\u0432\u0430\u0440\u0430."
    - "\u0410 \u0442\u0435\u0431\u0435 \u0435\u0441\u0442\u044C \u043A\u0443\u0434\u0430
      \u043A\u043B\u0430\u0441\u0442\u044C \u0442\u043E\u0432\u0430\u0440?"
    - "\u041E\u0441\u0432\u043E\u0431\u043E\u0434\u0438 \u043C\u0435\u0441\u0442\u043E
      \u0438 \u0432\u043E\u0437\u0432\u0440\u0430\u0449\u0430\u0439\u0441\u044F."
